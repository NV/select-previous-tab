{
	"name": "Select Previous Tab",
	"version": "1.0.0",
	"description": "Adds a keyboard shortcut to select the previously viewed tab.",
	"manifest_version": 3,
	"icons": {
		"16": "icons/16-light.png",
		"32": "icons/32-light.png",
		"48": "icons/48-light.png",
		"128": "icons/128-light.png"
	},
	"permissions": [],
	"background": {
		"service_worker": "background.js"
	},
	"action": {},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Alt+Q",
				"mac": "MacCtrl+Q"
			}
		}
	},
	"minimum_chrome_version": "121",
	"update_url": "http://clients2.google.com/service/update2/crx"
}
